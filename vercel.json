{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["data/**", "config/**", "models/**", "controllers/**", "routes/**", "middlewares/**"],
        "installCommand": "npm install --no-optional"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
